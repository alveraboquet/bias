   "source": [
    "import panel as pn\n",
    "import numpy as np\n",
    "import bias_SCANNER_FTX_MAIN as bot\n",
    "import sys\n",
    "\n",
    "def start_button(event):\n",
    "    bot.main(date_picker.value.day, date_picker.value.month, date_picker.value.year)\n",
    "    \n",
    "pn.extension(sizing_mode=\"stretch_width\")\n",
    "\n",
    "date_picker = pn.widgets.DatePicker(name='Select a Date to RUN Backtest')\n",
    "button = pn.widgets.Button(name='START', button_type='primary')\n",
    "terminal = pn.widgets.Terminal(\n",
    "    \"Bot Terminal\",\n",
    "    options={\"cursorBlink\": True},\n",
    "    height=300, sizing_mode='stretch_width'\n",
    ")\n",
    "\n",
    "button.on_click(start_button)\n",
    "dashboard = pn.Column(date_picker, button, terminal)\n",
    "sys.stdout = terminal\n",
    "\n",
    "pn.template.FastListTemplate(\n",
    "    site=\"Panel\", \n",
    "    title=\"FTX PERPETUAL-BIAS SCANNER\", \n",
    "    main=[\n",
    "        dashboard,\n",
    "    ], main_max_width=\"768px\",\n",
    ").servable();"
   ]
  }
